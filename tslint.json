{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "server"
    ]
  },
  "rules": {
    "prefer-for-of": false,
    "grouped-imports": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "jsx-no-lambda": false,
    "no-console": false,
    "only-arrow-functions": false
  }
}
